{
   "name": "nodejs-boilarplate-typescript",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "clean": "rimraf dist",
      "build": "tsc -v && npm run clean && npm run tslint && npm run build-ts",
      "build-ts": "tsc",
      "watch": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
      "tslint": "tslint -c tslint.json -p tsconfig.json",
      "tslint:fix": "tslint --fix src/**/*.{ts,js}",
      "test": "NODE_ENV=test mocha -r ts-node/register src/tests/*.spec.ts --exit"
   },
   "keywords": ["nodejs", "typescript", "boilarplate", "typeorm"],
   "author": "Md. Alamin",
   "license": "ISC",
   "devDependencies": {
      "@types/chai": "^4.1.5",
      "@types/chai-http": "^3.0.5",
      "@types/cors": "^2.8.9",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.11",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/mocha": "^8.2.0",
      "@types/morgan": "^1.9.2",
      "@types/node": "^8.0.29",
      "chai": "^4.1.2",
      "chai-http": "^4.2.0",
      "mocha": "^5.2.0",
      "rimraf": "^2.6.2",
      "sequelize-cli": "^6.2.0",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333",
      "tslint": "^5.11.0"
   },
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "mysql": "^2.14.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "sequelize": "^6.4.0",
      "typeorm": "0.2.30",
      "winston": "^3.3.3"
   }
}
